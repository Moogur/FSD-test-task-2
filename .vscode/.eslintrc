{
  "extends":"airbnb-base",
  "parser": "babel-eslint",
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "semi": ["error", "always", {"omitLastInOneLineBlock": true}],
    "comma-dangle": ["error", "never"],
    "no-console": "off",
    "max-len": ["error", {"code": 120}]
  }
}